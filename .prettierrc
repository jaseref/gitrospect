{
    "useTabs": false,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "none",
    "printWidth": 100,
    "semi": true,
    "endOfLine": "lf",
    "plugins": ["prettier-plugin-svelte"],
    "overrides": [
        { "files": "*.svelte", "options": { "parser": "svelte" } },
        { "files": "*.json", "options": { "tabWidth": 2 } }
    ]
}
